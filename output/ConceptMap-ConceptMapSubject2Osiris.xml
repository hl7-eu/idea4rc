  <targetUri value="http://hl7.eu/fhir/ig/idea4rc/StructureDefinition/Osiris"/>
  <group>
    <source value="http://hl7.eu/fhir/ig/idea4rc/StructureDefinition/Subject"/>
    <target value="http://hl7.eu/fhir/ig/idea4rc/StructureDefinition/Osiris"/>
    <element>
      <code value="Subject"/>
      <display value="Subject of care"/>
      <target>
        <code value="Osiris.Patient"/>
        <display value="Patient"/>
        <equivalence value="equivalent"/>
      </target>
    </element>
    <element>
      <code value="Subject.dateOfBirth"/>
