        <equivalence value="equivalent"/>
      </target>
    </element>
    <element>
      <code value="Subject.dateOfBirth.unknown"/>
      <display value="Date of birth day unknown"/>
      <target>
        <equivalence value="unmatched"/>
      </target>
    </element>
    <element>
      <code value="Subject.maritalStatus"/>
      <display value="Marital status"/>
      <target>
        <equivalence value="unmatched"/>
      </target>
    </element>
    <element>
      <code value="Subject.sexOrGender"/>
      <display value="Gender/Sex"/>
      <target>
        <code value="Osiris.Patient.Gender"/>
        <display value="Gender [C2]"/>
        <equivalence value="narrower"/>
      </target>
    </element>
    <element>
      <code value="Subject.race"/>
      <display value="Race"/>
      <target>
        <code value="Osiris.Patient.Ethnicity"/>
        <display value="Ethnicity [C3]"/>
        <equivalence value="wider"/>
      </target>
    </element>
    <element>
      <code value="Subject.countryOfResidence"/>
      <display value="Country of Residence "/>
      <target>
        <equivalence value="unmatched"/>
      </target>
    </element>
  </group>
